<h1>Log In</h1>
<%= form_for(:user, url: login_path) do |f| %>
<div class="field">
<%= f.label :name %><br>
<%= f.text_field :name %>
</div>
 <div class="field">
   <%= f.label :password %><br>
<%= f.password_field :password %>
</div>
 <div class="actions">
  <%= f.submit "Log In" %>
</div>
<% end %>
